{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zfcampus/zf-development-mode": "^3.0",
        "zendframework/zend-authentication":"*",
        "zendframework/zend-test":"*",
        "zendframework/zend-barcode":"*",
        "zendframework/zend-cache":"*",
        "zendframework/zend-captcha":"*",
        "zendframework/zendcloud":"*",
        "zendframework/zend-config":"*",
        "zendframework/zend-console":"*",
        "zendframework/zend-crypt":"*",
        "zendframework/zend-db":"*",
        "zendframework/zend-debug":"*",
        "zendframework/zend-di":"*",
        "zendframework/zend-code":"*",
        "zendframework/zend-dom":"*",
        "zendframework/zend-escaper":"*",
        "zendframework/zend-eventmanager":"*",
        "zendframework/zend-feed":"*",
        "zendframework/zend-file":"*",
        "zendframework/zend-filter":"*",
        "zendframework/zend-form":"*",
        "zendframework/zendgdata":"*",
        "zendframework/zend-http":"*",
        "zendframework/zend-i18n":"*",
        "zendframework/zend-inputfilter":"*",
        "zendframework/zend-json":"*",
        "zendframework/zend-ldap":"*",
        "zendframework/zend-loader":"*",
        "zendframework/zend-log":"*",
        "zendframework/zend-mail":"*",
        "zendframework/zend-math":"*",
        "zendframework/zend-memory":"*",
        "zendframework/zend-mime":"*",
        "zendframework/zend-modulemanager":"*",
        "zendframework/zend-navigation":"*",
        "zendframework/zendoauth":"*",
        "zendframework/zendopenid":"*",
        "zendframework/zend-paginator":"*",
        "zendframework/zendpdf":"*",
        "zendframework/zend-permissions-acl":"*",
        "zendframework/zend-permissions-rbac":"*",
        "zendframework/zend-progressbar":"*",
        "zendframework/zendrest":"*",
        "zendframework/zend-serializer":"*",
        "zendframework/zend-server":"*",
        "zendframework/zendservice-agilezen":"*",
        "zendframework/zendservice-akismet":"*",
        "zendframework/zendservice-amazon":"*",
        "zendframework/zendservice-gogrid":"*",
        "zendframework/zendservice-livedocx":"*",
        "zendframework/zend-servicemanager":"*",
        "zendframework/zendservice-nirvanix":"*",
        "zendframework/zendservice-rackspace":"*",
        "zendframework/zendservice-recaptcha":"*",
        "zendframework/zendservice-slideshare":"*",
        "zendframework/zendservice-strikeiron":"*",
        "zendframework/zendservice-technorati":"*",
        "zendframework/zendservice-twitter":"*",
        "zendframework/zendservice-windowsazure":"*",
        "zendframework/zend-session":"*",
        "zendframework/zend-soap":"*",
        "zendframework/zend-stdlib":"*",
        "zendframework/zend-tag":"*",
        "zendframework/zend-text":"*",
        "zendframework/zend-uri":"*",
        "zendframework/zend-validator":"*",
        "zendframework/zend-version":"*",
        "zendframework/zend-view":"*",
        "zendframework/zend-xmlrpc":"*"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Album\\": "module/Album/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
    }
}
